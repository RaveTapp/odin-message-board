<html>
  <p>Hello from index</p>
  <ul>
    <% for (let i = 0; i < messages.length; i++) { %>
    <li>
      <p>
        <i><%= messages[i].text %></i> from <span> <%= messages[i].user %> at </span><%=
        messages[i].added %> <a href="/messages/<%= i %>">open</a>
      </p>
    </li>
    <% } %>
  </ul>
  <a href="/new">New message</a>
</html>
