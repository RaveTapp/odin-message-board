<html>
  <h1>Message board</h1>
  <ul>
    <% for (let i = 0; i < messages.length; i++) { %>
    <li>
      <p>
        <i><%= messages[i].text %></i> from
        <span> <%= messages[i].user %></span>
        <a href="/message/<%= i %>">open</a>
      </p>
    </li>
    <% } %>
  </ul>
  <a href="/new">New message</a>
</html>
